%table.table
  %thead
    %tr
      %th Email
      %th Ingresos
      %th Ultimo Ingreso
      %th Acciones
  %tbody
    - @users.each do |user|
      %tr.user
        %td= user.email
        %td= user.login_count
        %td= user.last_login_at.try(:to_s, :short)
        %td.toggle= toggle_access_link_for(user)
